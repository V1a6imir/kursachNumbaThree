<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kur3.client.controller.ObjectCharacteristicsController">
   <children>
      <TableView fx:id="showCharacteristicsLabel" prefHeight="200.0" prefWidth="376.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="addressColumn" prefWidth="75.0" text="АДРЕС" />
            <TableColumn fx:id="userColumn" prefWidth="75.0" text="ПОЛЬЗОВАТЕЛЬ" />
            <TableColumn fx:id="workColumn" prefWidth="75.0" text="ВИД РАБОТ" />
            <TableColumn fx:id="objIdColumn" prefWidth="75.0" text="ID ОКН" />
        </columns>
      </TableView>
      <TextField fx:id="addressTextField" layoutX="14.0" layoutY="262.0" promptText="введите адрес..." />
      <TextField fx:id="typeOfWorkTextField" layoutX="14.0" layoutY="327.0" promptText="введите тип работ..." />
      <TextField fx:id="userTextField" layoutX="14.0" layoutY="295.0" promptText="введите пользователя..." />
      <TextField fx:id="objectIdTextField" layoutX="14.0" layoutY="360.0" promptText="введите id..." />
      <Button fx:id="handleAddButton" layoutX="14.0" layoutY="218.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="26.0" prefWidth="55.0" text="Add" />
      <Button fx:id="handleBackButton" layoutX="532.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButton" text="Назад" />
      <Button fx:id="handleDeleteButton" layoutX="319.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleDeleteButton" prefHeight="26.0" prefWidth="55.0" text="Delete" />

   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
